<h1>All Shelters</h1>

<h2><% @shelters.each do |shelter|  %></h2>
<h3><%= shelter.name %></h3>
<%= link_to "Edit #{shelter.name}'s Info", "/shelters/#{shelter.id}/edit" %>
<p><%= link_to "Delete #{shelter.name}", "/shelters/#{shelter.id}", method: :delete %></p>
<p>------</p>
<% end %>

<p><%= link_to "New Shelter", "/shelters/new" %></p>
